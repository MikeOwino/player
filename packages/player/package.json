{
  "name": "@vidstack/player",
  "version": "1.4.0",
  "description": "Headless web components that make integrating media on the a web a breeze.",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=14.19.0"
  },
  "files": [
    "dist/",
    "dist-node/",
    "dist-prod/",
    "dist-cdn/",
    "define/",
    "react/",
    "svelte/",
    "index.d.ts",
    "globals.d.ts",
    "elements.json",
    "tailwind.cjs",
    "vscode.html-data.json"
  ],
  "sideEffects": [
    "./define/*.js",
    "./src/define/*.ts",
    "./dist/define/*.js",
    "./dist-prod/define/*.js",
    "./dist-cdn/define/*.js"
  ],
  "exports": {
    ".": {
      "node": "./dist-node/index.js",
      "development": "./dist/index.js",
      "default": "./dist-prod/index.js"
    },
    "./define/*": {
      "node": "./dist-node/define/*",
      "development": "./dist/define/*",
      "default": "./dist-prod/define/*"
    },
    "./cdn/define/*": {
      "default": "./dist-cdn/define/*"
    },
    "./react": {
      "node": "./dist-node/react/index.js",
      "development": "./dist/react/index.js",
      "default": "./dist-prod/react/index.js"
    },
    "./svelte": {
      "node": "./dist-node/svelte/node/index.js",
      "development": "./dist/svelte/client/index.js",
      "default": "./dist-prod/svelte/client/index.js"
    },
    "./package.json": "./package.json",
    "./tailwind.cjs": "./tailwind.cjs",
    "./elements.json": "./elements.json",
    "./vscode.html-data.json": "./vscode.html-data.json"
  },
  "contributors": [
    "Dave Furfero <furf@furf.com>",
    "Rahim Alwer <rahim.alwer@gmail.com>",
    "Sam Potts <sam@potts.es>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vidstack/vidstack.git",
    "directory": "packages/player"
  },
  "bugs": {
    "url": "https://github.com/vidstack/vidstack/issues"
  },
  "scripts": {
    "analyze": "wireit",
    "build": "wireit",
    "build:dist": "wireit",
    "build:definitions": "wireit",
    "build:types": "wireit",
    "extract:types": "wireit",
    "extract:types:react": "wireit",
    "extract:types:svelte": "wireit",
    "clean": "rimraf define dist dist-* types coverage src/react/components src/svelte/{client,node}/components",
    "dev": "pnpm build watch",
    "watch": "pnpm dev",
    "lint:eslint": "wireit",
    "lint:prettier": "wireit",
    "minify:cdn": "wireit",
    "sandbox": "pnpm sandbox:prepare && vite --open /sandbox/index.html --port 3091",
    "sandbox:prepare": "node ../../.scripts/copy.js --entry=.templates/sandbox --outdir=sandbox --overwrite=false",
    "test": "wireit",
    "test:watch": "vitest --watch"
  },
  "wireit": {
    "analyze": {
      "dependencies": [
        "../eliza:build",
        "../foundation:extract:types"
      ],
      "files": [
        "src/**",
        "tsconfig.json",
        "eliza.config.ts"
      ],
      "command": "eliza \"src/**/*{events,Element}.ts\"",
      "output": [
        "react/index.js",
        "svelte/index.js",
        "elements.json",
        "vscode.html-data.json"
      ],
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "build": {
      "dependencies": [
        "build:dist",
        "build:definitions",
        "extract:types",
        "extract:types:react",
        "extract:types:svelte",
        "minify:cdn"
      ]
    },
    "build:dist": {
      "dependencies": [
        "analyze",
        "../foundation:build:dist"
      ],
      "files": [
        "src/**",
        "tsconfig.json",
        "rollup.config.mjs",
        "../rollup/*.js"
      ],
      "command": "rollup -c",
      "output": [
        "dist/**",
        "dist-node/**",
        "dist-cdn/**",
        "dist-prod/**"
      ],
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "build:types": {
      "dependencies": [
        "analyze",
        "../foundation:extract:types"
      ],
      "clean": "if-file-deleted",
      "files": [
        "src/**",
        "eliza.config.ts",
        "tsconfig.json",
        "../eliza/dist-node/**",
        "../foundation/types/**"
      ],
      "command": "tsc -p tsconfig.json --pretty && rimraf dist/**/*.d.ts",
      "output": [
        "types/**",
        "tsconfig.tsbuildinfo"
      ],
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "build:definitions": {
      "dependencies": [
        "build:types"
      ],
      "files": [
        "src/define/**"
      ],
      "command": "node ../../.scripts/definitions.js",
      "output": [
        "define/**"
      ]
    },
    "extract:types": {
      "dependencies": [
        "build:types"
      ],
      "files": [
        "types/**",
        "tsconfig.json"
      ],
      "command": "api-extractor run",
      "output": [
        "index.d.ts"
      ],
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "extract:types:react": {
      "dependencies": [
        "build:types"
      ],
      "files": [
        "types/**",
        "tsconfig.json"
      ],
      "command": "api-extractor run -c api-extractor.react.json",
      "output": [
        "react/index.d.ts"
      ],
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "extract:types:svelte": {
      "dependencies": [
        "build:types"
      ],
      "files": [
        "types/**",
        "tsconfig.json"
      ],
      "command": "node ./.scripts/build-svelte-types.js && api-extractor run -c api-extractor.svelte.json",
      "output": [
        "svelte/index.d.ts"
      ],
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "minify:cdn": {
      "dependencies": [
        "build:dist"
      ],
      "files": [
        "dist-cdn/**"
      ],
      "command": "node ../../.scripts/minify.js --entry=dist-cdn/**/*.js --outdir=dist-cdn",
      "output": [
        "dist-cdn/**"
      ],
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "lint:eslint": {
      "files": [
        "src/**"
      ],
      "command": "eslint src --fix",
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "lint:prettier": {
      "files": [
        "src/**"
      ],
      "command": "prettier src --write --loglevel warn",
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    },
    "test": {
      "files": [
        "src/**",
        "tests/**",
        "vite.config.ts"
      ],
      "command": "vitest --run",
      "packageLocks": [
        "pnpm-lock.yaml"
      ]
    }
  },
  "dependencies": {
    "@vidstack/foundation": "workspace:*"
  },
  "peerDependencies": {
    "lit": "^2.1.1"
  },
  "devDependencies": {
    "@lit/reactive-element": "^1.3.1",
    "@microsoft/api-extractor": "^7.20.0",
    "@open-wc/semantic-dom-diff": "^0.19.5",
    "@open-wc/testing-helpers": "^2.1.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/lru-cache": "^7.4.0",
    "@types/react": "^17.0.39",
    "@vidstack/eliza": "workspace:*",
    "@vidstack/rollup": "workspace:*",
    "autoprefixer": "^10.4.2",
    "hls.js": "^1.1.3",
    "jsdom": "^19.0.0",
    "lit": "^2.1.3",
    "lit-html": "^2.2.1",
    "lru-cache": "^7.4.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "svelte": "^3.45.0",
    "tailwindcss": "^3.0.22",
    "tslib": "^2.3.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.1",
    "vitest": "^0.6.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "accessible",
    "audio",
    "customizable",
    "dailymotion",
    "elements",
    "extensible",
    "file",
    "framework-agnostic",
    "hls",
    "html5",
    "lit",
    "media",
    "plyr",
    "player",
    "react",
    "spec-compliant",
    "universal",
    "vds",
    "video",
    "vidstack",
    "vime",
    "vimeo",
    "web-component",
    "universal",
    "youtube"
  ]
}
